<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="308.0" prefWidth="487.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.teamname.views.EmergencyController">
   <children>
      <VBox alignment="CENTER" prefHeight="308.0" prefWidth="487.0" style="-fx-background-color: #ff0000;">
         <children>
            <Label prefHeight="103.0" prefWidth="250.0" text="CALL HELP?" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font size="46.0" />
               </font>
            </Label>
            <JFXButton fx:id="yesBtn" onAction="#popupYes" prefHeight="63.0" prefWidth="174.0" style="-fx-background-color: #ffffff; -fx-background-radius: 15px;" text="YES">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
               <font>
                  <Font size="27.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="noButton" onAction="#close" prefHeight="33.0" prefWidth="84.0" style="-fx-background-color: #ffffff; -fx-background-radius: 10px;" text="NO" />
         </children>
      </VBox>
      <StackPane fx:id="stackPane" layoutX="133.0" layoutY="110.0" prefHeight="103.0" prefWidth="229.0" />
   </children>
</AnchorPane>
