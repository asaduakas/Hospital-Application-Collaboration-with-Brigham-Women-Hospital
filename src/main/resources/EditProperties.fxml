   <children>
      <Pane prefHeight="94.0" prefWidth="402.0" style="-fx-background-color: #043d7e;">
         <children>
            <ImageView fitHeight="94.0" fitWidth="196.0">
               <image>
                  <Image url="@hospital_Logo.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <GridPane hgap="10.0" layoutX="21.0" layoutY="118.0" prefHeight="252.0" prefWidth="361.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="195.0" minWidth="10.0" prefWidth="181.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="293.0" minWidth="10.0" prefWidth="180.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <JFXTextField fx:id="nodeID" promptText="NodeID">
               <effect>
                  <Glow level="0.5" />
               </effect>
            </JFXTextField>
            <JFXTextField fx:id="building" promptText="Building" GridPane.rowIndex="2">
               <effect>
                  <Glow level="0.5" />
               </effect>
            </JFXTextField>
            <JFXTextField fx:id="floor" promptText="Floor" GridPane.rowIndex="1">
               <effect>
                  <Glow level="0.5" />
               </effect>
            </JFXTextField>
            <JFXTextField fx:id="nodeType" promptText="Node Type" GridPane.columnIndex="1">
               <effect>
                  <Glow level="0.5" />
               </effect>
            </JFXTextField>
            <JFXTextField fx:id="longName" promptText="LongName" GridPane.columnIndex="1" GridPane.rowIndex="1">
