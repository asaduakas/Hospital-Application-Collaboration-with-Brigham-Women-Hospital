<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>


<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.d21.teamD.views.NotificationController">
   <children>
      <Pane fx:id="anotherPane">
         <children>
            <JFXButton fx:id="closeBtn" text="Close" translateX="130.0" translateY="10.0" underline="true">
               <font>
                  <Font size="15.0" />
               </font>
            </JFXButton>
            <ScrollPane fx:id="scrollPane" fitToWidth="true" layoutY="50.0">
               <content>
                  <TextFlow fx:id="textFlow" prefWidth="200.0" />
               </content>
            </ScrollPane>
         </children>
      </Pane>
   </children>
</AnchorPane>
